{
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "stylelint.validate": ["css", "scss"],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true
  },
  "editor.fontSize": 15,
  "workbench.iconTheme": "material-icon-theme",
  "editor.tabSize": 2,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.printWidth": 300,
  "editor.renderWhitespace": "all",
  "editor.cursorWidth": 2,
  "html.format.wrapLineLength": 0,
  "prettier.singleQuote": true,
  "editor.links": false,
  "editor.mouseWheelZoom": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.scrollBeyondLastLine": false,
  "editor.smoothScrolling": true,
  "editor.scrollbar.verticalScrollbarSize": 0,
  "editor.scrollbar.vertical": "hidden",
  "problems.showCurrentInStatus": true,
  "clock.dateFormat": "HH:MM:ss",
  "atomKeymap.promptV3Features": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorWidth": 2,
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.lineHeight": 1.2,
  "workbench.tree.renderIndentGuides": "always",
  "vsicons.presets.hideExplorerArrows": true,
  "vsicons.presets.jsOfficial": true,
  "vsicons.presets.jsonOfficial": true,
  "vsicons.presets.tsOfficial": true,
  "material-icon-theme.activeIconPack": "nest",
  "material-icon-theme.saturation": 0.8,
  "material-icon-theme.folders.theme": "none",
  "material-icon-theme.hidesExplorerArrows": true,
  "workbench.tree.indent": 12,
  "git.showPushSuccessNotification": true,
  "git-graph.graph.colours": ["#81c08b", "#ea902f", "#78b0a0", "#d84a4a", "#8fb0a9", "#386e46", "#f5b58c", "#ffd152", "#d9008f", "#a300d9", "#e138e8", "#6f24d6"],
  "diffEditor.renderSideBySide": false,
  "git.closeDiffOnOperation": true,
  "git.confirmSync": false,
  "scm.inputFontSize": 14,
  "debug.console.fontSize": 15,
  "workbench.startupEditor": "none",
  "vscode-edge-devtools.mirrorEdits": true,
  "tabnine.experimentalAutoImports": true,
  "highlight-matching-tag.highlightFromContent": true,
  "highlight-matching-tag.styles": {
    "opening": {
      "name": {
        "highlight": "#ffd15266"
      }
    }
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "window.autoDetectHighContrast": false,
  "workbench.editor.untitled.hint": "hidden",
  "editor.wordWrap": "on",
  "breadcrumbs.enabled": false,
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "diffEditor.renderIndicators": false,
  "scm.diffDecorationsGutterWidth": 4,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "disable-color-correct-rendering": false,
  "explorer.confirmDragAndDrop": false,
  "git.enableSmartCommit": true,
  "explorer.confirmDelete": false,
  "html.format.enable": false,
  "editor.formatOnSave": true,
  "html.hover.references": false,
  "css.hover.references": false,
  "less.hover.references": false,
  "scss.hover.references": false,
  "html.hover.documentation": false,
  "css.hover.documentation": false,
  "less.hover.documentation": false,
  "scss.hover.documentation": false,
  "zenMode.hideLineNumbers": false,
  "workbench.colorTheme": "Ushi",
  "editor.renderLineHighlight": "none",
  "editor.accessibilitySupport": "off",
  "editor.autoClosingQuotes": "beforeWhitespace",
  "editor.codeLens": false,
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.guides.bracketPairs": true,
  "editor.guides.bracketPairsHorizontal": false,
  "vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
  "vscode-neovim.neovimInitVimPaths.darwin": "/Users/ushimaru/.config/nvim/init.vim",
  "[vue]": {
    "editor.defaultFormatter": "Vue.volar"
  },
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "turboConsoleLog.includeFileNameAndLineNum": false,
  "turboConsoleLog.insertEnclosingClass": false,
  "turboConsoleLog.insertEnclosingFunction": false,
  "turboConsoleLog.delimiterInsideMessage": "",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[scss]": {
    "editor.defaultFormatter": "vscode.css-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  }
}
