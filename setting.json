{
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "stylelint.validate": ["css", "scss"],
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true
  },
  "editor.fontSize": 15,
  "workbench.iconTheme": "material-icon-theme",
  "editor.tabSize": 2,
  "editor.wordWrap": "on",
  "editor.renderWhitespace": "all",
  "editor.cursorWidth": 2,
  "editor.minimap.showSlider": "always",
  "zenMode.hideLineNumbers": false,
  "zenMode.hideTabs": false,
  "html.format.wrapLineLength": 0,
  "prettier.singleQuote": true,
  "editor.links": false,
  "editor.mouseWheelZoom": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.scrollBeyondLastLine": false,
  "editor.smoothScrolling": true,
  "editor.renderLineHighlight": "none",
  "workbench.colorTheme": "Ushi",
  "editor.scrollbar.verticalScrollbarSize": 0,
  "editor.scrollbar.vertical": "hidden",
  "problems.showCurrentInStatus": true,
  "clock.dateFormat": "HH:MM:ss",
  "atomKeymap.promptV3Features": true,
  "editor.formatOnPaste": true,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorWidth": 2,
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.lineHeight": 1.2,
  "workbench.tree.renderIndentGuides": "always",
  "vsicons.presets.hideExplorerArrows": true,
  "vsicons.presets.jsOfficial": true,
  "vsicons.presets.jsonOfficial": true,
  "vsicons.presets.tsOfficial": true,
  "material-icon-theme.activeIconPack": "nest",
  "material-icon-theme.saturation": 0.8,
  "material-icon-theme.folders.theme": "none",
  "editor.minimap.renderCharacters": false,
  "material-icon-theme.hidesExplorerArrows": true,
  "workbench.tree.indent": 12,
  "git.showPushSuccessNotification": true,
  "git-graph.graph.colours": [
    "#81c08b",
    "#ea902f",
    "#78b0a0",
    "#d84a4a",
    "#8fb0a9",
    "#386e46",
    "#f5b58c",
    "#ffd152",
    "#d9008f",
    "#a300d9",
    "#e138e8",
    "#6f24d6"
  ],
  "breadcrumbs.enabled": false,
  "diffEditor.renderSideBySide": false,
  "diffEditor.renderIndicators": false,
  "git.closeDiffOnOperation": true,
  "git.confirmSync": false,
  "scm.inputFontSize": 14,
  "debug.console.fontSize": 15,
  "workbench.startupEditor": "none",
  "vscode-edge-devtools.mirrorEdits": true,
  "tabnine.experimentalAutoImports": true,
  "highlight-matching-tag.highlightFromContent": true,
  "highlight-matching-tag.styles": {
    "opening": {
      "name": {
        "highlight": "#ffd15266"
      }
    }
  },
  "editor.bracketPairColorization.enabled": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "html.format.enable": false,
  "javascript.format.enable": false
}
